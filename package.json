{
  "name": "ng-extract-i18n-reverse",
  "version": "2.7.0",
  "description": "Extract and reverse-merge i18n AMB translation files for angular projects.",
  "main": "index.js",
  "module": "index.mjs",
  "builders": "builders.json",
  "schematics": "./schematics/collection.json",
  "type": "module",
  "ng-add": {
    "save": "devDependencies"
  },
  "ng-update": {
    "migrations": "./schematics/migrations/migrations.json"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc",
    "prepack": "tsc",
    "test": "jest",
    "test-coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daniel-sc/ng-extract-i18n-reverse.git"
  },
  "keywords": [
    "angular",
    "i18n",
    "amb"
  ],
  "author": "Timur Mishagin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daniel-sc/ng-extract-i18n-reverse/issues"
  },
  "homepage": "https://github.com/ndmsystems/ng-extract-i18n-reverse#readme",
  "dependencies": {
    "@angular-devkit/architect": "^0.1600.6",
    "@angular-devkit/core": "^16.1.6",
    "@angular-devkit/schematics": "^16.1.6",
    "@schematics/angular": "^16.1.6",
    "terminal-link": "^3.0.0",
    "xliff-simple-merge": "~1.0.2",
    "xml_normalize": "^1.0.0",
    "xmldoc": "~1.1.4"
  },
  "peerDependencies": {
    "@angular-devkit/build-angular": "^13.0.0 || ^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@types/jest": "~29.5.3",
    "@types/xmldoc": "~1.1.6",
    "jest": "~28.1.3",
    "ts-jest": "~28.0.8",
    "ts-node": "~10.9.1",
    "tslint": "~6.1.3",
    "typescript": "^5.1.6"
  }
}
